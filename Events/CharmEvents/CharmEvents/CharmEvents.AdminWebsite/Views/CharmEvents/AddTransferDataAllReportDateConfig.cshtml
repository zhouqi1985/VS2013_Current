@using ObjectUtil.Common
@using ObjectUtil.Dictionary
@using CharmEvents.Database.Database
@model TransferData_All_ReportDateConfig

@{
    ViewBag.Title = "编辑";
}

@section title
{
    <script type="text/javascript">
		$(document).ready(function()
		{
		    //singer 选中变换class
			$('.form .left, .form input, .form textarea, .form select').focus(function(){
				$(this).parents('tr').addClass("over");
			}).blur(function(){
				$(this).parents('tr').removeClass("over");
			});

		});
    </script>
}

@Html.HeadBegin("编辑")
@Html.HeadEnd()


<!--- data op --->
<div id="data">
    @using (Html.BeginForm())
    {
        <table width="100%" border="0" cellspacing="0" class="table-data form">
            <!------------>
            <!--  -->
            <tr style="display:none;">
                <th width="20%">ConfigId:</th>
                <td width="80%">
                    @Html.TextBoxFor(m => Model.ConfigId, new { Class = "input", Size = "40" })
                    @Html.JQueryValidationMessage(j => j.Bind("ConfigId").NotEmpty())
                </td>
            </tr>
            <!--  -->
            <tr>
                <th width="20%">ConfigName:</th>
                <td width="80%">
                    @Html.TextBoxFor(m => Model.ConfigName, new { Class = "input", Size = "40" })
                    @Html.JQueryValidationMessage(j => j.Bind("ConfigName").NotEmpty())
                </td>
            </tr>
            <!--  -->
            <tr>
                <th width="20%">Status:</th>
                <td width="80%">
                    @Html.TextBoxFor(m => Model.Status, new { Class = "input", Size = "40" })
                    @Html.JQueryValidationMessage(j => j.Bind("Status").NotEmpty())
                </td>
            </tr>
            <!-- (1)GameDetailsPerUser(2)ItemConsume(3)TotalPeople -->
            <tr>
                <th width="20%">ReportId</th>
                <td width="80%">
                    @Html.TextBoxFor(m => Model.ReportId, new { Class = "input", Size = "40" })
                    @Html.JQueryValidationMessage(j => j.Bind("ReportId").NotEmpty())
                    (1)GameDetailsPerUser(2)ItemConsume(3)TotalPeople
                </td>
            </tr>
            <!--  -->
            <tr>
                <th width="20%">StartTime:</th>
                <td width="80%">
                    @Html.TextBoxWithCalendar("StartTime", null, new { Class = "input", Size = "40" }, "yy-mm-dd")
                    @Html.JQueryValidationMessage(j => j.Bind("StartTime").NotEmpty())
                </td>
            </tr>
            <!--  -->
            <tr>
                <th width="20%">EndTime:</th>
                <td width="80%">
                    @Html.TextBoxWithCalendar("EndTime", null, new { Class = "input", Size = "40" }, "yy-mm-dd")
                    @Html.JQueryValidationMessage(j => j.Bind("EndTime").NotEmpty())
                </td>
            </tr>
            <!--  -->
            <tr>
                <th width="20%">CreateTime:</th>
                <td width="80%">
                    @Html.TextBoxFor(m => Model.CreateTime, new { Class = "input", Size = "40" })
                    @Html.JQueryValidationMessage(j => j.Bind("CreateTime").NotEmpty())
                </td>
            </tr>
            <tr>
                <th>&nbsp;</th>
                <td height="50" class="clear-form-td">
                    <input type="submit" class="bt-big" name="button2" id="button2" value="提  交" />
                    <input type="button" name="goback" class="bt-big" value="返  回" onclick="javascript:history.go(-1);" />
                </td>
            </tr>
            <!------------>
        </table>

    }

</div>
<!--- data ed --->
@Html.Foot("ReportId:(1)GameDetailsPerUser(2)ItemConsume(3)TotalPeople")

